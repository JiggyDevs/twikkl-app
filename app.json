{
  "expo": {
    "owner": "twikkl",
    "name": "twikkl-app",
    "slug": "twikkl-app",
    "scheme": "twikkl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/logos/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/imgs/logos/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/logos/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.twikkl.twikklapp",
      "permissions": ["CAMERA", "RECORD_AUDIO", "WRITE_EXTERNAL_STORAGE"]
    },
    "extra": {
     
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/95c0ba49-9d63-43af-94f1-39d5e26ea120"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow twikkl to access your camera.",
          "microphonePermission": "Allow twikkl to access your microphone"
        }
      ]
    ]
  }
}
